sition(updateDom);\n\ncelsiusInput.addEventListener(\"input\", (e) =>\n  service.send({\n    type: \"UPDATE_FAHRENHEIT\",\n    value: e.target.value,\n  })\n);\n\nfahrenheitInput.addEventListener(\"input\", (e) =>\n  service.send({\n    type: \"UPDATE_CELSIUS\",\n    value: e.target.value,\n  })\n);\n"},"lineCount":null}},"error":null,"hash":"e658364e08b812d40f81927eecc31f11","cacheData":{"env":{}}}